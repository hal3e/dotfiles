{
  "keymaps": {
    "0": { "type": "scroll.home" },
    ":": { "type": "command.show" },
    "p": { "type": "command.show.open", "alter": false },
    "P": { "type": "command.show.open", "alter": true },
    "b": { "type": "command.show.tabopen", "alter": false },
    "B": { "type": "command.show.tabopen", "alter": true },
    "d": { "type": "command.show.winopen", "alter": false },
    "D": { "type": "command.show.winopen", "alter": true },
    "v": { "type": "command.show.buffer" },
    "e": { "type": "scroll.vertically", "count": -1 },
    "n": { "type": "scroll.vertically", "count": 1 },
    "y": { "type": "scroll.horizonally", "count": -1 },
    "o": { "type": "scroll.horizonally", "count": 1 },
    "<C-U>": { "type": "scroll.pages", "count": -0.5 },
    "<C-D>": { "type": "scroll.pages", "count": 0.5 },
    "<C-B>": { "type": "scroll.pages", "count": -1 },
    "<C-F>": { "type": "scroll.pages", "count": 1 },
    "gg": { "type": "scroll.top" },
    "G": { "type": "scroll.bottom" },
    "$": { "type": "scroll.end" },
    "h": { "type": "tabs.close" },
    "f": { "type": "tabs.reopen" },
    "N": { "type": "tabs.prev", "count": 1 },
    "E": { "type": "tabs.next", "count": 1 },
    "g0": { "type": "tabs.first" },
    "g$": { "type": "tabs.last" },
    "w": { "type": "tabs.reload", "cache": false },
    "W": { "type": "tabs.reload", "cache": true },
    "z;": { "type": "tabs.pin.toggle" },
    "zh": { "type": "tabs.duplicate" },
    "zu": { "type": "zoom.in" },
    "zp": { "type": "zoom.out" },
    "zz": { "type": "zoom.neutral" },
    "t": { "type": "follow.start", "newTab": false },
    "T": { "type": "follow.start", "newTab": true },
    "Y": { "type": "navigate.history.prev" },
    "O": { "type": "navigate.history.next" },
    "[[": { "type": "navigate.link.prev" },
    "]]": { "type": "navigate.link.next" },
    "gu": { "type": "navigate.parent" },
    "gU": { "type": "navigate.root" },
    "gs": { "type": "page.source" },
    "y": { "type": "urls.yank" },
    "/": { "type": "find.start" },
    "k": { "type": "find.next" },
    "K": { "type": "find.prev" },
    "l": { "type": "mark.set.prefix" },
    "'": { "type": "mark.jump.prefix" },
    "<S-Esc>": { "type": "addon.toggle.enabled" }
  },
  "search": {
    "default": "g",
    "engines": {
      "g": "https://google.com/search?q={}",
      "so": "https://stackoverflow.com/search?q={}",
      "w": "https://en.wikipedia.org/w/index.php?search={}"
    }
  },
  "properties": {
    "hintchars": "ansehoti",
    "smoothscroll": true,
    "complete": "sbh"
  }
}
